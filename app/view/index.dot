<html>
<head>
	<meta charSet="utf-8" />
	<title>{{=it.title}}</title>
	<base href="{{=it.appRoot}}/">
	<meta host="{{= it.host }}">
	<meta httpEquiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0"/>
	<meta name="apple-mobile-web-app-capable" content="yes"/>
	<meta name="mobile-web-app-capable" content="yes"/>
	{{? it.cssPath}}
	<link rel="stylesheet" type="text/css" href={{=it.cssPath}}></link>
	{{?}}
	<script src="{{=it.appRoot}}/assets/polyfills.js"></script>
	<script src="{{=it.appRoot}}/assets/stats.min.js"></script>
	<link rel="shortcut icon" href="{{=it.appRoot}}/assets/favicon.ico" type="image/x-icon">
</head>
<body>
	<div id="app" class="layout"></div>
	<script>var initialAppState = {{=it.user}};</script>
	<script src="{{=it.scriptSrc}}"></script>
</body>
</html>
